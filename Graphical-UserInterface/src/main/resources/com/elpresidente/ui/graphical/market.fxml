<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.elpresidente.ui.graphical.MarketController">
   <children>
      <Label fx:id="label" text="Label" />
      <HBox maxHeight="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
         <children>
            <TextField fx:id="textField" onAction="#textFiledAction" />
            <Button fx:id="buyButton" mnemonicParsing="false" onAction="#buy" text="Buy" />
         </children>
      </HBox>
   </children>
</VBox>
